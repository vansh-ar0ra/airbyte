{
  "documentationUrl": "https://docs.airbyte.com/integrations/destinations/rabbitmq",
  "supported_destination_sync_modes": ["append"],
  "supportsIncremental": true,
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Destination Rabbitmq",
    "type": "object",
    "required": ["host", "pod_URL", "data_mapping", "username", "password"],
    "additionalProperties": false,
    "properties": {
      "pod_URL": {
        "type": "string",
        "description": "URL for posting the data from the queue after data mapping."
      },
      "host": {
        "type": "string",
        "description": "The RabbitMQ host name."
      },
      "username": {
        "type": "string",
        "description": "The username to connect."
      },
      "password": {
        "type": "string",
        "title": "Password",
        "description": "The password to connect.",
        "airbyte_secret": true
      },
      "data_mapping": {
        "title": "Data Mapping",
        "type": "string",
        "description": "Data Mapping of the fields from the Source Connector to the Pod Schema as a json object",
        "default": ""
      },
      "webhookURL": {
        "type": "string",
        "description": "Webhook URL to be passed to the Pod, which receives notification about the job status"
      }
    }
  }
}
